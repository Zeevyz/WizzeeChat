<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="wizzeechat">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_wizzee_company_square"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_wizzee_company_square"
        android:supportsRtl="true"
        android:theme="@style/Theme.ZeeWizChat">
        <activity
            android:name="com.example.wizzeechat.FindUserActivity"
            android:exported="false" />
        <activity
            android:name="com.example.wizzeechat.MainPageActivity"
            android:exported="false" />
        <activity
            android:name="com.example.wizzeechat.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>